[ rdfs:comment "Component in error in a binding -> Comp Spec associated through binding not available" ;
   rdfs:label "S10" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate tomasys:binding_component ;
                           swrl:argument1 <http://ros/navigation#b> ;
                           swrl:argument2 <http://ros/navigation#c>
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate tomasys:binding_role ;
                                      swrl:argument1 <http://ros/navigation#b> ;
                                      swrl:argument2 <http://ros/navigation#r>
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate tomasys:roleDef ;
                                                 swrl:argument1 <http://ros/navigation#r> ;
                                                 swrl:argument2 <http://ros/navigation#cs>
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate tomasys:Binding ;
                                                            swrl:argument1 <http://ros/navigation#b>
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                                       swrl:propertyPredicate tomasys:c_status ;
                                                                       swrl:argument1 <http://ros/navigation#c> ;
                                                                       swrl:argument2 "false"^^xsd:boolean
                                                                     ] ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate tomasys:cspec_availability ;
                           swrl:argument1 <http://ros/navigation#cs> ;
                           swrl:argument2 "false"^^xsd:boolean
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ rdfs:comment "binding in ERROR/false in an fg -> fg in ERROR/false" ;
   rdfs:label "S3" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate tomasys:Binding ;
                           swrl:argument1 <http://ros/navigation#b>
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate tomasys:hasBindings ;
                                      swrl:argument1 <http://ros/navigation#fg> ;
                                      swrl:argument2 <http://ros/navigation#b>
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                 swrl:propertyPredicate tomasys:b_status ;
                                                 swrl:argument1 <http://ros/navigation#b> ;
                                                 swrl:argument2 "false"^^xsd:boolean
                                               ] ;
                                     rdf:rest rdf:nil
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate tomasys:fg_status ;
                           swrl:argument1 <http://ros/navigation#fg> ;
                           swrl:argument2 "IN_ERROR_COMPONENT"
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ rdfs:comment "if a component of a unique ComponentClass is in ERROR, the class is unavailable" ;
   rdfs:label "S4" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate tomasys:cc_unique ;
                           swrl:argument1 tomasys:cc ;
                           swrl:argument2 "true"^^xsd:boolean
