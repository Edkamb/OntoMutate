#################################################################
#    Object Properties
#################################################################

###  http://metacontrol.org/tomasys#binding_component
tomasys:binding_component rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain tomasys:Binding ;
                          rdfs:range tomasys:Component .


###  http://metacontrol.org/tomasys#binding_role
tomasys:binding_role rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tomasys:Binding ;
                     rdfs:range tomasys:Role .


###  http://metacontrol.org/tomasys#fd_error_log
tomasys:fd_error_log rdf:type owl:ObjectProperty ;
                     rdfs:domain tomasys:FunctionDesign ;
                     rdfs:range tomasys:Objective ;
                     rdfs:comment "contains all objectives for which an FG instance of the FD has failed" .


###  http://metacontrol.org/tomasys#hasBindings
tomasys:hasBindings rdf:type owl:ObjectProperty ;
                    rdfs:domain tomasys:FunctionGrounding ;
                    rdfs:range tomasys:Binding .


###  http://metacontrol.org/tomasys#hasNFR
tomasys:hasNFR rdf:type owl:ObjectProperty ;
               rdfs:domain tomasys:Objective ;
               rdfs:range tomasys:QAvalue .


###  http://metacontrol.org/tomasys#hasQAestimation
tomasys:hasQAestimation rdf:type owl:ObjectProperty ;
                        rdfs:domain tomasys:FunctionDesign ;
                        rdfs:range tomasys:QAvalue .


###  http://metacontrol.org/tomasys#hasQAvalue
tomasys:hasQAvalue rdf:type owl:ObjectProperty ;
                   rdfs:domain tomasys:FunctionGrounding ;
                   rdfs:range tomasys:QAvalue .


###  http://metacontrol.org/tomasys#isQAtype
tomasys:isQAtype rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tomasys:QAvalue ;
                 rdfs:range tomasys:QualityAttributeType .


###  http://metacontrol.org/tomasys#requiresO
tomasys:requiresO rdf:type owl:ObjectProperty ;
                  rdfs:domain tomasys:FunctionGrounding ;
                  rdfs:range tomasys:Objective ;
                  rdfs:comment "A FunctionGrounding may need an Objective to be solved (by someone else) as a requirement to realise its target Objective" .


###  http://metacontrol.org/tomasys#roleDef
tomasys:roleDef rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain tomasys:Role ;
                rdfs:range tomasys:Component ,
                           tomasys:ComponentSpecification ;
                rdfs:comment "a Role consists of the definition of a component specification for a Function Design" .


###  http://metacontrol.org/tomasys#roles
tomasys:roles rdf:type owl:ObjectProperty ;
              rdfs:domain tomasys:FunctionDesign ;
              rdfs:range tomasys:Role ;
              rdfs:comment "A Function Design consists of roles" .


###  http://metacontrol.org/tomasys#solvesF
tomasys:solvesF rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain tomasys:FunctionDesign ;
                rdfs:range tomasys:Function ;
                rdfs:comment "a FunctionDesign solves a Function" .


###  http://metacontrol.org/tomasys#solvesO
tomasys:solvesO rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain tomasys:FunctionGrounding ;
                rdfs:range tomasys:Objective ;
                rdfs:comment "A FunctionGrounding is realising an Objective" .


###  http://metacontrol.org/tomasys#typeC
tomasys:typeC rdf:type owl:ObjectProperty ;
              rdfs:domain tomasys:Component ;
              rdfs:range tomasys:ComponentClass .


###  http://metacontrol.org/tomasys#typeF
tomasys:typeF rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain tomasys:Objective ;
              rdfs:range tomasys:Function ;
              rdfs:comment "relates a FunctionGrounding/Objective with a FunctionDesign/Function" .


###  http://metacontrol.org/tomasys#typeFD
tomasys:typeFD rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain tomasys:FunctionGrounding ;
               rdfs:range tomasys:FunctionDesign .


###  http://ros/mros#requiredBy
mros:requiredBy rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://metacontrol.org/tomasys#b_status
tomasys:b_status rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tomasys:Binding ;
                 rdfs:range xsd:boolean .


###  http://metacontrol.org/tomasys#c_status
tomasys:c_status rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tomasys:Component ,
                             tomasys:ComponentState ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "FALSE" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "RECOVERED" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "TRUE" ;
                                                                rdf:rest rdf:nil
                                                              ]
                                                   ]
                                        ]
                            ] ;
                 rdfs:comment "operational status of a component. For the moment \"false\"=\"ERROR\", later we can expand the status to an enumeration" .


###  http://metacontrol.org/tomasys#cc_availability
tomasys:cc_availability rdf:type owl:DatatypeProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain tomasys:ComponentClass ;
                        rdfs:range xsd:boolean ;
                        rdfs:comment """there is engineering information about a Component Class that may dy-
namically vary during system operation, and that is the availability [0, 1] of that type of component. For example, a sensor can be restarted/re-deployed to fix a wrong initial configuration of one of its parameters. However, no matter what reconfiguration we may carry, if there is only one physical sensor in the system, and it is broken, the component instance will not work: it is unavailable.""" .


###  http://metacontrol.org/tomasys#cc_unique
tomasys:cc_unique rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain tomasys:ComponentClass ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "if a Component Class is unique, then only a single instance may exist, and if in error, then the Class becomes unavailable" .


###  http://metacontrol.org/tomasys#cspec_availability
tomasys:cspec_availability rdf:type owl:DatatypeProperty ;
                           rdfs:domain tomasys:ComponentSpecification ;
                           rdfs:range xsd:boolean ;
                           rdfs:comment """A particular ComponentSpecification might not be available.
However, this does not mean that other Configurations of that component are not available (i.e. the ComponentClass is unavailable)
Example: the Yumi configuration with two arms might not be available, but it may be available single-arm""" .


###  http://metacontrol.org/tomasys#fd_efficacy
tomasys:fd_efficacy rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain tomasys:FunctionDesign ;
                    rdfs:range xsd:decimal .


###  http://metacontrol.org/tomasys#fd_realisability
tomasys:fd_realisability rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain tomasys:FunctionDesign ;
                         rdfs:range xsd:boolean .


###  http://metacontrol.org/tomasys#fg_status
tomasys:fg_status rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain tomasys:FunctionGrounding ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "IN_ERROR_COMPONENT" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "IN_ERROR_FR" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "IN_ERROR_NFR" ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ]
                             ] .


###  http://metacontrol.org/tomasys#hasValue
tomasys:hasValue rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tomasys:QAvalue ;
                 rdfs:range xsd:decimal .


###  http://metacontrol.org/tomasys#o_always_improve
tomasys:o_always_improve rdf:type owl:DatatypeProperty ;
                         rdfs:domain tomasys:Objective ;
                         rdfs:range xsd:boolean .


###  http://metacontrol.org/tomasys#o_status
tomasys:o_status rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tomasys:Objective ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "IN_ERROR_COMPONENT" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "IN_ERROR_FR" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "IN_ERROR_NFR" ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "IN_PROGRESS" ;
                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                      rdf:first "UNGROUNDED" ;
                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                 rdf:first "UNREACHABLE" ;
                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                            rdf:first "UPDATABLE" ;
                                                                                                            rdf:rest rdf:nil
                                                                                                          ]
                                                                                               ]
                                                                                    ]
                                                                         ]
                                                              ]
                                                   ]
                                        ]
                            ] .


###  http://metacontrol.org/tomasys#qa_comparison_operator
tomasys:qa_comparison_operator rdf:type owl:DatatypeProperty ;
                               rdfs:domain tomasys:QualityAttributeType ;
                               rdfs:range [ rdf:type rdfs:Datatype ;
                                            owl:oneOf [ rdf:type rdf:List ;
                                                        rdf:first "<" ;
                                                        rdf:rest [ rdf:type rdf:List ;
                                                                   rdf:first "<=" ;
                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                              rdf:first ">" ;
                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                         rdf:first ">=" ;
                                                                                         rdf:rest rdf:nil
                                                                                       ]
                                                                            ]
                                                                 ]
                                                      ]
                                          ] .


###  http://metacontrol.org/tomasys#qa_critical
tomasys:qa_critical rdf:type owl:DatatypeProperty ;
                    rdfs:domain tomasys:QualityAttributeType ;
                    rdfs:range xsd:boolean .


