[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "if component recovery, objective updatable"^^xsd:string ;
   rdfs:label "compRecov"^^xsd:string ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate tomasys:Component ;
                           swrl:argument1 mros:c
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                      swrl:propertyPredicate tomasys:c_status ;
                                      swrl:argument1 mros:c ;
                                      swrl:argument2 "RECOVERED"^^xsd:string
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate tomasys:Objective ;
                                                 swrl:argument1 mros:o
                                               ] ;
                                     rdf:rest rdf:nil
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate tomasys:o_status ;
                           swrl:argument1 mros:o ;
                           swrl:argument2 "UPDATABLE"^^xsd:string
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "if component required for FD not working, FD not available"^^xsd:string ;
   rdfs:label "compError"^^xsd:string ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate tomasys:Component ;
                           swrl:argument1 mros:c
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate mros:requiredBy ;
                                      swrl:argument1 mros:c ;
